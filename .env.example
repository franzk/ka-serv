# Copier ce fichier en .env et adapter les valeurs
# ATTENTION : Ne jamais commiter le fichier .env

# ===========================================
# ka-front
# ===========================================
VITE_KEYCLOAK_REALM=kaserv
VITE_KEYCLOAK_CLIENT_ID=ka-front
VITE_API_URL=https://api.change.me/api

# ===========================================
# ka-keycloak
# ===========================================
KC_DB_PASSWORD=change_me_db_password
KC_BOOTSTRAP_ADMIN_USERNAME=change_me_admin
KC_BOOTSTRAP_ADMIN_PASSWORD=change_me_strong_password
KEYCLOAK_HOST=auth.change.me

# (build-time) utilis√© par ka-front via docker-compose
KEYCLOAK_URL=https://auth.change.me

KEYCLOAK_ISSUER=https://auth.change.me/realms/kaserv

# ===========================================
# ka-gateway
# ===========================================
BACK_URL=http://ka-back:5027
APP_URL=https://app.change.me

# ===========================================
# ka-mailer
# ===========================================
SMTP_HOST=smtp.change-my-provider.com
SMTP_PORT=465
SMTP_USERNAME=change_me@change.me
SMTP_PASSWORD=CHANGE_ME
SMTP_AUTH=true
SMTP_SSL=true

# ===========================================
# ka-smtp-bridge
# ===========================================
MAILER_URL=http://ka-mailer:5030

# Optional: if set, ka-serv services will also join this external docker network (reverse proxy)
PROXY_NETWORK_NAME=